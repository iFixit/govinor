// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}

model Branch {
  name                   String @id
  handle                 String @unique
  cloneUrl               String
  dockerComposeDirectory String
}
